{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "nuxt build -a",
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore . --ignore-pattern **/*.min.js",
    "lint:style": "stylelint \"assets/**/*.scss\" --syntax scss",
    "fix": "npm run fix:js && npm run fix:style",
    "fix:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore . --ignore-pattern **/*.min.js",
    "fix:style": "stylelint \"assets/**/*.scss\" --syntax scss --fix",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "precommit": [
    "precommit"
  ],
  "lint-staged": {
    "*.(js|vue)": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --syntax scss --fix"
    ]
  },
  "browserslist": [
    "cover 95% in ID"
  ],
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@privyid/eslint-config-js": "^5.0.0",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.15.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-adenvt": "^4.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "eslint-plugin-varspacing": "^1.2.2",
    "eslint-plugin-vue": "^7.16.0",
    "fibers": "^5.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.12",
    "pre-commit": "^1.2.2",
    "sass": "^1.38.0",
    "sass-loader": "^10.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1"
  }
}
